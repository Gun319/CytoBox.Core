{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "Enable": "Sqlite",
    "SqlServer": "Server=(LocalDb)\\MSSQLLocalDB;Database=CytoBox;Trusted_Connection=True;TrustServerCertificate=True",
    "MySql": "Server=localhost;Port=3306;Database=CytoBox;Uid=root;Pwd=123456;CharSet=utf8;",
